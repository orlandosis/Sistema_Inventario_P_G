
@using CNTI365.FACTUR.WEB.Helpers
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Sistema de Inventario</title>

    <!-- CSS -->
    <link href="~/Content/plugins/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/css/bootstrap-select.min.css" rel="stylesheet" />
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap-glyphicons.css" rel="stylesheet">
    <link href="~/Content/plugins/css/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/plugins/dataTables/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/dataTables/css/responsive.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/css/ionicons.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <link href="~/Content/plugins/sweetalert2/sweetalert.css" rel="stylesheet" />
    <link href="~/Content/plugins/alertifyjs/css/alertify.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/select2/css/select2.min.css" rel="stylesheet" />
    <link href="~/Content/style/style.css" rel="stylesheet" />
    

    <!-- JS -->
    <script type="text/javascript" src="~/Content/plugins/js/jquery-3.1.1.min.js"></script>
    <script  type="text/javascript" src="~/Content/plugins/js/bootstrap.min.js"></script>
    <script  type="text/javascript" src="~/Content/plugins/js/jquery-ui.js"></script>
    <script  type="text/javascript" src="~/Content/plugins/select2/js/select2.min.js"></script>
    <script  type="text/javascript" src="~/Content/plugins/dataTables/js/jquery.dataTables.min.js"></script>
    <script  type="text/javascript" src="~/Content/plugins/dataTables/js/dataTables.bootstrap.min.js"></script>
    <script  type="text/javascript" src="~/Content/plugins/dataTables/js/dataTables.responsive.min.js"></script>
    <script  type="text/javascript" src="~/Content/plugins/dataTables/js/responsive.bootstrap.min.js"></script>
    <script  type="text/javascript" src="~/Content/plugins/sweetalert2/sweetalert.js"></script>
    <script  type="text/javascript" src="~/Content/plugins/alertifyjs/alertify.min.js"></script>

    <script  type="text/javascript" type="text/javascript" src="~/Scripts/js/global.js"></script>
    <script  type="text/javascript" type="text/javascript" src="~/Scripts/js/table.js"></script>
    <script  type="text/javascript" src="https://apis.google.com/js/platform.js"></script>
    <script>
        var appURL = "@Url.Content("~/")";
    </script>

</head>
<body>
    
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                <span class="sr-only">Desplegar navegacion</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#"><img src="~/Content/img/libros.png" height="35" /></a>
        </div>

        <div class="collapse navbar-collapse navbar-ex1-collapse">
            <ul class="nav navbar-nav navbar-right">
                <li><a href="#" onclick="proveedores();">Proveedores</a></li>
                <li><a href="#" onclick="productos();">Productos</a></li>
                <li><a href="#" onclick="inventario();">Inventario</a></li>
                <li><a href="#" onclick="empleados();">Empleados</a></li>
                <li><a href="#" onclick="clientes();">Clientes</a></li>
                <li><a href="#" onclick="compras();">Compras</a></li>
                <li><a href="#" onclick="ventas();">Ventas</a></li>
                <li><a href="#" onclick="dashboard();">Dashboard</a></li>
                <li><a href="#" onclick="registro();">Registro de Usuario</a></li>
                <li><a href="#" onclick="panel();">Panel Principal</a></li>
                <li><a href="#">Bienvenido:</a></li>
                <li class="dropdown">

                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        @Session.GetCurrentUser().username <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu">
                        @*<li><a href="#">Mi cuenta</a></li>*@
                        <li class="divider"></li>
                        <li><a href="#" onclick="cerrarsession();">Cerrar sesión</a></li>
                    </ul>

                </li>
            </ul>
        </div>
    </nav>




    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>

            <div class="col-md-12">

                <div class="col-md-4">
                    <p>&copy; @DateTime.Now.Year - Licencia activa</p>
                </div>
                <div class="col-md-4">
                    @*<div class="g-ytsubscribe" data-channelid="UCALemX0WOTvmGiBFQhgA9xw" data-layout="default" data-count="default"></div>*@
                </div>

            </div>
            
            

            
        </footer>
    </div>

 
</body>
</html>

<script>
    function cerrarsession() {
        window.location = fnBaseURLWeb("Home/Exit");
    }

    function proveedores() {
        window.location = fnBaseURLWeb("Proveedores/Proveedores");
    }
    function productos() {
        window.location = fnBaseURLWeb("Productos/Productos");
    }
    function inventario() {
        window.location = fnBaseURLWeb("Inventarios/Inventarios");
    }
    function empleados() {
        window.location = fnBaseURLWeb("Empleados/Empleados");
    }
    function panel() {
        window.location = fnBaseURLWeb("Panel/Panel");
    }
    function registro() {
        window.location = fnBaseURLWeb("RegistroEmpresa/RegistroEmpresa");
    }
    function clientes() {
        window.location = fnBaseURLWeb("Clientes/Clientes");
    }
    function compras() {
        window.location = fnBaseURLWeb("Compras/Compras");
    }
    function ventas() {
        window.location = fnBaseURLWeb("Ventas/Ventas");
    }
    function dashboard() {
        window.location = fnBaseURLWeb("Dashboard/Dashboard");
    }


    
</script>
